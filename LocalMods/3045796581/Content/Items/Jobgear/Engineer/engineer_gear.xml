        <StatusEffect type="OnWearing" target="Character" PressureProtection="6000.0" setvalue="true" disabledeltatime="true">
          <Conditional IsPlayer="eq false" />
        </StatusEffect>
        <!-- For singleplayer compatibility | Provide oxygen for bots without reducing oxygen tank condition -->
        <StatusEffect type="OnWearing" target="Character" OxygenAvailable="1000.0" comparison="And">
          <Conditional IsDead="false" />
          <Conditional IsPlayer="eq false" />
          <RequiredItem items="oxygensource" type="Contained" />
        </StatusEffect>
        <!-- Prevent the wearer from breathing hull oxygen, requiring them to use oxygen tanks | Disabled for bots -->
        <StatusEffect type="OnWearing" target="Character" OxygenAvailable="-100.0" UseHullOxygen="false">
          <Conditional IsPlayer="eq true" />
        </StatusEffect>
        <!-- Provide oxygen to wearer if any oxygensource is contained and drain the oxygen tank as fast as abyss diving suit | Disabled for bots -->
        <StatusEffect type="OnWearing" target="Contained,Character" OxygenAvailable="1000.0" Condition="-0.15" comparison="And">
          <Conditional IsDead="false" />
          <Conditional IsPlayer="eq true" />
          <RequiredItem items="oxygensource" type="Contained" />
        </StatusEffect>
        <!-- Suffocate the wearer if welding fuel is inserted -->
        <StatusEffect type="OnWearing" target="Contained,Character" Oxygen="-10.0" Condition="-0.5" interval="1" disabledeltatime="true">
          <Conditional IsDead="false" />
          <RequiredItem items="weldingfueltank" type="Contained" />
        </StatusEffect>
        <!-- Suffocate and burn the wearer if incendium fuel is inserted -->
